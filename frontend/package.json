{
  "name": "careconnect-frontend",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "npm run validate-startup && next start",
    "start:production": "NODE_ENV=production npm run validate-startup && next start",
    "validate-startup": "node scripts/validate-startup.js",
    "test:regression": "powershell -ExecutionPolicy Bypass -File ../scripts/critical-path-regression-tests.ps1 -Quick",
    "test:regression:full": "powershell -ExecutionPolicy Bypass -File ../scripts/critical-path-regression-tests.ps1",
    "test:regression:demo": "powershell -ExecutionPolicy Bypass -File ../scripts/critical-path-regression-tests.ps1 -DemoMode",
    "lint": "next lint",
    "typecheck:v2": "tsc --noEmit -p tsconfig.v2.json",
    "format:check:v2": "npx prettier --check \"app/onboarding/v2/**/*.{ts,tsx}\" \"app/profile/**/*.{ts,tsx}\" \"components/**/*.{ts,tsx}\"",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:shims": "node scripts/create-test-shims.js",
    "ga:debug:wizard": "node scripts/debug_onboarding_v2_client_error.mjs"
  },
  "dependencies": {
    "@headlessui/react": "^1.7.17",
    "@heroicons/react": "^2.0.18",
    "@supabase/supabase-js": "^2.38.5",
    "@tanstack/react-query": "^5.8.4",
    "@types/qrcode": "^1.5.6",
    "@types/react": "^18.2.39",
    "@types/react-dom": "^18.2.17",
    "autoprefixer": "^10.4.16",
    "axios": "^1.6.2",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.0.0",
    "framer-motion": "^10.16.16",
    "lucide-react": "^0.294.0",
    "next": "14.0.3",
    "postcss": "^8.4.32",
    "qrcode": "^1.5.4",
    "react": "^18.2.0",
    "react-audio-visualize": "^1.0.2",
    "react-dom": "^18.2.0",
    "react-hot-toast": "^2.4.1",
    "react-query": "^3.39.3",
    "recharts": "^3.5.1",
    "tailwindcss": "^3.3.6",
    "typescript": "^5.3.2",
    "zustand": "^4.4.7"
  },
  "devDependencies": {
    "@tailwindcss/forms": "^0.5.10",
    "@tailwindcss/typography": "^0.5.19",
    "@testing-library/jest-dom": "^6.1.5",
    "@testing-library/react": "^14.1.2",
    "@testing-library/user-event": "^14.5.1",
    "@types/node": "^20.9.4",
    "eslint": "^8.54.0",
    "eslint-config-next": "14.0.3",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "playwright": "^1.58.2"
  }
}
